<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="whorlview_style"><attr format="string" name="whorlview_circle_colors"/><attr format="integer" name="whorlview_circle_speed"/><attr name="whorlview_parallax">
            <enum name="fast" value="1"/>
            <enum name="medium" value="0"/>
            <enum name="slow" value="2"/>
        </attr><attr format="float" name="whorlview_sweepAngle"/><attr format="float" name="whorlview_strokeWidth"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="app_name">小车遥控</string>
    <string name="copyright">小组成员：海伊斯、梁娟、张鹏</string>
    <string name="ellipsis">......</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.



        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="MyToggleButton" parent="@android:style/Widget.CompoundButton">
        <item name="android:button">@drawable/toggle_status_changed</item>
    </style>
</resources>